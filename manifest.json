{
  "name": "Tab Switcher",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "switch-last": {
      "suggested_key": {
        "default": "Ctrl+Shift+Right"
      },
      "description": "Switch between last two tabs"
    }
  }
}